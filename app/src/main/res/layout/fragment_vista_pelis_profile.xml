<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_2"
    tools:context=".fragments.VistaPelisProfileFragment">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView14"
                        style="@style/my_movies_profile"
                        android:layout_width="180dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli5" />

                    <ImageView
                        android:id="@+id/imageView13"
                        style="@style/my_movies_profile"
                        android:layout_width="180dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli4" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView12"
                        style="@style/my_movies_profile"
                        android:layout_width="192dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli3" />

                    <ImageView
                        android:id="@+id/imageView15"
                        style="@style/my_movies_profile"
                        android:layout_width="192dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli6" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView17"
                        style="@style/my_movies_profile"
                        android:layout_width="192dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli8" />

                    <ImageView
                        android:id="@+id/imageView11"
                        style="@style/my_movies_profile"
                        android:layout_width="192dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli2" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView10"
                        style="@style/my_movies_profile"
                        android:layout_width="192dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli1" />

                    <ImageView
                        android:id="@+id/imageView16"
                        style="@style/my_movies_profile"
                        android:layout_width="192dp"
                        android:layout_height="250dp"
                        android:src="@drawable/peli7" />

                </TableRow>
            </TableLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>